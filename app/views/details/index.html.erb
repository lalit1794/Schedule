<p id="notice"><%= notice %></p>

<h1>Details</h1>

<table>
  <thead>
    <tr>
      <th>Loan</th>
      <th>Term</th>
      <th>Rate</th>
      <th>Disburse date</th>
      <th>Pay strt date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @details.each do |detail| %>
      <tr>
        <td><%= detail.loan %></td>
        <td><%= detail.term %></td>
        <td><%= detail.rate %></td>
        <td><%= detail.disburse_date %></td>
        <td><%= detail.pay_strt_date %></td>
        <td><%= link_to 'Show', detail %></td>
        <td><%= link_to 'Edit', edit_detail_path(detail) %></td>
        <td><%= link_to 'Destroy', detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Detail', new_detail_path %>
